{
    "ipcMode": null,
    "executionRoleArn": "arn:aws:iam::741566723297:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::slackscheduler/.env",
          "type": "s3"
       }
